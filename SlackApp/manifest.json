{
    "display_information": {
        "name": "RBot",
        "description": "Engineering Resource Bot",
        "background_color": "#222324"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": true,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "RBot",
            "always_online": true
        },
        "assistant_view": {
            "assistant_description": "This agent can find engineering resources.",
            "suggested_prompts": []
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "assistant:write",
                "chat:write",
                "im:history",
                "channels:history",
                "reactions:read",
                "commands",
                "app_mentions:read",
                "users:read",
                "reactions:write",
                "files:read",
                "files:write",
                "mpim:history"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_mention",
                "assistant_thread_started",
                "message.channels",
                "message.im",
                "message.mpim"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}